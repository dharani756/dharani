#include<stdio.h>
int main()
{
  int a[20],i,num,pos=0,n;
  printf("enter array size");
  scanf("%d", &n);
  printf("enter the elements");
  for(i=0;i<n;i++)
  {
    scanf("%d",&a[i]);
  }  
  printf("enter the element to be search");
 scanf("%d",&num);
  for(i=0;i<n;i++)
  {


   if(num==a[i])
   {
    pos=1;
     break;

    }
   } 
 if(pos==1)
 {

  printf("found at index%d",pos);
 }
 return 0;
}







