import java.lang.*;
class My
{
public static void main(String[]args)
{
try
{
int a[]={10,20,30};
System.out.println(a[14]);
}
catch(ArithmeticException ae)
{
System.out.println(ae);
}
catch(ArrayIndexOutOfBoundsException v)
{
System.out.println(v);
}
catch(Exception e)
{
System.out.println(e);
}
}

}
